pipeline {
    agent any

    tools {
        jdk 'java17'
        maven 'mymaven'
    }

    stages {
        stage('Checkout') {
            steps {
                git 'https://github.com/Jaswanth8106/Devops-Project.git'
            }
        }

        stage('Build') {
            steps {
                sh 'mvn clean package'
            }
        }
    }
}
